<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/acumuladoDuplaSena"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/background_telas_jogos"
    android:scrollbars="none" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="699dp"
        android:paddingLeft="@dimen/activity_vertical_margin"
        android:paddingRight="@dimen/activity_horizontal_margin" >

        <RelativeLayout
            android:id="@+id/Layout_duplasena"
            android:layout_width="match_parent"
            android:layout_height="730dp"
            android:paddingLeft="@dimen/activity_vertical_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:visibility="visible" >

            <RelativeLayout
                android:id="@+id/relativeLayoutDuplaSena"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="@dimen/content"
                android:layout_marginRight="@dimen/content"
                android:layout_marginTop="10dp"
                android:background="@drawable/content3"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/resultadoConcursoDuplaSena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/white"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/andetonha"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:text="@string/resultado_duplasena"
                    android:textColor="@color/white"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

            </RelativeLayout>

            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_below="@+id/relativeLayoutDuplaSena"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="@dimen/content"
                android:layout_marginRight="@dimen/content"
                android:layout_marginTop="5dp"
                android:background="@drawable/content3"
                android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/concurso_lotofacil"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/premiacao1"
                    android:textStyle="bold"
					android:textColor="@color/white"
                    android:textAppearance="?android:attr/textAppearanceMedium" />
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/relativeLayoutDuplaSena2"
                android:layout_width="match_parent"
                android:layout_height="190dp"
                android:layout_alignLeft="@+id/relativeLayoutDuplaSena"
                android:layout_below="@+id/linearLayout1"
                android:layout_marginTop="5dp"
                android:background="@drawable/content2" >

                <TextView
                    android:id="@+id/numero_concursoDuplaSena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="2dp"
                    android:gravity="center"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textStyle="bold"
					android:textColor="@color/black" />

                <TextView
                    android:id="@+id/concurso_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/numero_concursoDuplaSena"
                    android:layout_marginTop="6dp"
                    android:gravity="center"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/premio_duplasenaLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/concurso_duplasena"
                    android:layout_marginTop="10dp"
                    android:text="@string/sena"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/rateio_sena_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/ganhadores_sena_duplasena"
                    android:layout_alignParentRight="true"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <View
                    android:id="@+id/view1"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_alignLeft="@+id/premio_duplasenaLabel"
                    android:layout_below="@+id/premio_duplasenaLabel"
                    android:layout_marginTop="5dp"
                    android:background="@color/background_telas_jogos" />

                <TextView
                    android:id="@+id/ganhadores_sena_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/view1"
                    android:layout_centerHorizontal="true"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/concurso_quina"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/view1"
                    android:layout_below="@+id/view1"
                    android:layout_marginTop="10dp"
                    android:text="@string/quina"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/rateio_quina_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/ganhadores_quina_duplasena"
                    android:layout_alignBottom="@+id/ganhadores_quina_duplasena"
                    android:layout_alignParentRight="true"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/ganhadores_quina_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/concurso_quina"
                    android:layout_alignBottom="@+id/concurso_quina"
                    android:layout_centerHorizontal="true"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <View
                    android:id="@+id/view2"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/concurso_quina"
                    android:layout_marginTop="5dp"
                    android:background="@color/background_telas_jogos" />

                <TextView
                    android:id="@+id/premio_timemaniaLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/view2"
                    android:layout_marginTop="10dp"
                    android:text="@string/quadra"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/rateio_quadra_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/ganhadores_quadra_duplasena"
                    android:layout_alignParentRight="true"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/ganhadores_quadra_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/premio_timemaniaLabel"
                    android:layout_alignBottom="@+id/premio_timemaniaLabel"
                    android:layout_centerHorizontal="true"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <View
                    android:id="@+id/view3"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_alignLeft="@+id/premio_timemaniaLabel"
                    android:layout_below="@+id/premio_timemaniaLabel"
                    android:layout_marginTop="5dp"
                    android:background="@color/background_telas_jogos" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_below="@+id/relativeLayoutDuplaSena2"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="@dimen/content"
                android:layout_marginRight="@dimen/content"
                android:layout_marginTop="5dp"
                android:background="@drawable/content3"
                android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/premio_lotofacilLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/premiacao2"
                    android:textStyle="bold"
					android:textColor="@color/white"
                    android:textAppearance="?android:attr/textAppearanceMedium" />
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/Layout_megasena"
                android:layout_width="match_parent"
                android:layout_height="190dp"
                android:layout_alignRight="@+id/relativeLayoutDuplaSena2"
                android:layout_below="@+id/linearLayout2"
                android:layout_marginTop="5dp"
                android:background="@drawable/content2" >

                <TextView
                    android:id="@+id/premio_quinaLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <TextView
                    android:id="@+id/concurso_timemania"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/premio_quinaLabel"
                    android:layout_marginTop="10dp"
                    android:text="@string/sena"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/ganhadores_sena2_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/concurso_timemania"
                    android:layout_centerHorizontal="true"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/rateio_sena2_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/ganhadores_sena2_duplasena"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@+id/premio_quinaLabel"
                    android:gravity="right"
                    android:text="@string/vazio"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <View
                    android:id="@+id/view4"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignLeft="@+id/concurso_timemania"
                    android:layout_below="@+id/concurso_timemania"
                    android:layout_marginTop="5dp"
                    android:background="@color/background_telas_jogos" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view4"
                    android:layout_marginTop="10dp"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:text="@string/quina"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/rateio_quina2_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/ganhadores_quina2_duplasena"
                    android:layout_alignLeft="@+id/rateio_sena2_duplasena"
                    android:layout_alignRight="@+id/view5"
                    android:gravity="right"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view5"
                    android:layout_marginTop="10dp"
                    android:text="@string/quadra"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/rateio_quadra2_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/ganhadores_quadra2_duplasena"
                    android:layout_alignBottom="@+id/ganhadores_quadra2_duplasena"
                    android:layout_alignLeft="@+id/rateio_quina2_duplasena"
                    android:layout_alignRight="@+id/view6"
                    android:gravity="right"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <View
                    android:id="@+id/view6"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignLeft="@+id/textView16"
                    android:layout_below="@+id/textView16"
                    android:layout_marginTop="5dp"
                    android:background="@color/background_telas_jogos" />

                <View
                    android:id="@+id/view5"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignLeft="@+id/textView13"
                    android:layout_below="@+id/textView13"
                    android:layout_marginTop="5dp"
                    android:background="@color/background_telas_jogos" />

                <TextView
                    android:id="@+id/ganhadores_quina2_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/view5"
                    android:layout_centerHorizontal="true"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/ganhadores_quadra2_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/view6"
                    android:layout_centerHorizontal="true"
                    android:text="@string/vazio"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />

                <TextView
                    android:id="@+id/acumulado_duplasena"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:textStyle="bold"
					android:textColor="@color/black"
                    android:textAppearance="?android:attr/textAppearanceSmall" />
            </RelativeLayout>

            <ImageButton
                android:id="@+id/VerResultadoDuplaSena"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_below="@+id/Layout_megasena"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="50dp"
                android:background="@drawable/btn_redondo"
                android:src="@drawable/ic_action_refresh" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/errorDuplaSena"
            android:layout_width="match_parent"
            android:layout_height="@dimen/error_altura"
            android:layout_below="@+id/Layout_duplasena"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/topo"
            android:background="@drawable/btn"
            android:gravity="center"
            android:visibility="gone"
            android:orientation="vertical" >
            
            <TextView
                android:id="@+id/concurso_megasena_new"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/error"
                android:gravity="center"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:textAppearance="?android:attr/textAppearanceMedium" />
            
        </LinearLayout>
    </RelativeLayout>

</ScrollView>