<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="br.com.qx.andetonha.megasena.TelaPrincipal$PlaceholderFragment" >

    <TextView
        android:id="@+id/numero_concurso_LotoFacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="@string/concurso_data"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#920888" />

    <TextView
        android:id="@+id/resultado_concurso_LotoFacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/numero_concurso_LotoFacil"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="3dp"
        android:text="@string/resultado"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#920888"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/sena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/resultado_concurso_LotoFacil"
        android:layout_marginTop="44dp"
        android:text="@string/acertos15"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/ganhadores_acertos15_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/sena"
        android:layout_alignBottom="@+id/sena"
        android:layout_centerHorizontal="true"
        android:text="@string/ganhadores"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/rateio_acertos15_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ganhadores_acertos15_lotofacil"
        android:layout_alignParentRight="true"
        android:text="@string/R$"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/ganhadores_acertos14_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/quina"
        android:layout_alignRight="@+id/ganhadores_acertos15_lotofacil"
        android:text="@string/ganhadores"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/rateio_acertos14_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ganhadores_acertos14_lotofacil"
        android:layout_alignBottom="@+id/ganhadores_acertos14_lotofacil"
        android:layout_alignRight="@+id/rateio_acertos15_lotofacil"
        android:text="@string/R$"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/ganhadores_acertos13_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/quadra"
        android:layout_alignBottom="@+id/quadra"
        android:layout_alignRight="@+id/ganhadores_acertos14_lotofacil"
        android:text="@string/ganhadores"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/rateio_acertos13_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ganhadores_acertos13_lotofacil"
        android:layout_alignRight="@+id/rateio_acertos14_lotofacil"
        android:text="@string/R$"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <View
        android:id="@+id/view2"
        android:layout_width="fill_parent"
        android:layout_height="2px"
        android:layout_above="@+id/ganhadores_acertos15_lotofacil"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="10dp"
        android:background="#920888" />

    <TextView
        android:id="@+id/quina"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sena"
        android:layout_below="@+id/sena"
        android:text="@string/acertos14"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/quadra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/quina"
        android:layout_below="@+id/quina"
        android:text="@string/acertos13"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/sena2_duplasena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/quadra"
        android:layout_below="@+id/quadra"
        android:text="@string/acertos12"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/rateio_acertos12_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ganhadores_acertos12_lotofacil"
        android:layout_alignBottom="@+id/ganhadores_acertos12_lotofacil"
        android:layout_alignParentRight="true"
        android:text="@string/R$"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/ganhadores_acertos12_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/sena2_duplasena"
        android:layout_alignBottom="@+id/sena2_duplasena"
        android:layout_alignLeft="@+id/ganhadores_acertos13_lotofacil"
        android:text="@string/ganhadores"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/acertos3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sena2_duplasena"
        android:layout_below="@+id/sena2_duplasena"
        android:text="@string/acertos11"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/ganhadores_acertos11_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/acertos3"
        android:layout_alignBottom="@+id/acertos3"
        android:layout_alignLeft="@+id/ganhadores_acertos12_lotofacil"
        android:text="@string/ganhadores"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/rateio_acertos11_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ganhadores_acertos11_lotofacil"
        android:layout_alignBottom="@+id/ganhadores_acertos11_lotofacil"
        android:layout_alignParentRight="true"
        android:text="@string/R$"
        android:textColor="#920888"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <View
        android:id="@+id/view1"
        android:layout_width="fill_parent"
        android:layout_height="2px"
        android:layout_alignLeft="@+id/acertos3"
        android:layout_below="@+id/acertos3"
        android:layout_marginTop="10dp"
        android:background="#920888" />

    <Button
        android:id="@+id/VerResultadoLotoFacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp"
        android:background="#920888"
        android:height="40dp"
        android:onClick="getResultados"
        android:shadowColor="#000000"
        android:text="@string/buscarResultado"
        android:textColor="#ffffff"
        android:width="150dp" />

    <TextView
        android:id="@+id/acumulou_lotofacil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/view2"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#920888" />

</RelativeLayout>